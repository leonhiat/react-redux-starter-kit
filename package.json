{
  "name": "isomorphic-react-webpack-starter",
  "version": "0.0.0",
  "description": "TBD",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "compile": "webpack && npm run compile:cleanup",
    "compile:cleanup": "rm dist/client/index.html",
    "dev": "webpack-dev-server --port 3000 --hot --inline --colors --history-api-fallback",
    "dev:quiet": "webpack-dev-server --port 3000 --hot --inline --colors --no-info --history-api-fallback --quiet",
    "test": "npm run test:unit",
    "test:unit": "node ./node_modules/karma/bin/karma start",
    "deploy": "npm run test && npm run compile",
    "server:start": "node --harmony ./server/scripts/start"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "autoprefixer-core": "^5.2.1",
    "autoprefixer-loader": "^1.2.0",
    "babel": "^5.2.15",
    "babel-core": "^5.2.15",
    "babel-eslint": "^3.1.23",
    "babel-loader": "^5.0.0",
    "babel-runtime": "^5.4.7",
    "css-loader": "^0.12.0",
    "eslint": "^0.24.1",
    "eslint-loader": "^0.14.2",
    "extract-text-webpack-plugin": "^0.8.0",
    "html-webpack-plugin": "^1.3.0",
    "koa": "^0.21.0",
    "koa-jade": "^1.1.0",
    "koa-static": "^1.4.9",
    "node-sass": "^3.2.0",
    "object-assign": "^3.0.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "sass-loader": "^1.0.3",
    "style-loader": "^0.12.1",
    "webpack": "^1.9.0"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "karma": "^0.12.32",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.5.0",
    "karma-coverage": "^0.3.1",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-spec-reporter": "0.0.19",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "phantomjs-polyfill": "0.0.1",
    "react-hot-loader": "^1.2.7",
    "webpack-dev-server": "^1.8.2"
  }
}
